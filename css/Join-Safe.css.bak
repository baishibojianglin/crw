/*xinchouwang.cn CSS Document */

/*CSS Reset*/
body,div,p,ul,li,a{margin:0; padding:0;}
img{border:none;}

/*page CSS*/

#banner{display:block; width:100%; height:300px; margin:0 auto;min-width:1160px;}
#banner img{display:block; width:100%; height:300px;}

#content{display:block;overflow:hidden;width:1160px; margin:0 auto;font:14px "Microsoft YaHei"; line-height:24px;}
#leftArticle,#rightInfo{margin-bottom:200px;float:left; display:inline;}
#leftArticle{width:23%;}
#leftArticle img{padding:0 20px;}
#leftArticle ul li{height:42px;line-height:42px;font-size:13px;color:#666;}
#leftArticle ul li a{color:#E93639;text-decoration:none;}
#leftArticle ul li a span{color:#FFF;}

#rightInfo{width:76.8%;border-left:2px solid #CDCDCD;}
#rightInfo tr{margin-bottom:10px;}
#rightInfo td{padding:10px 0;}
table{padding:0 40px;}
.hr1{width:267px;height:2px;color:#F0F0F0;background-color:#F0F0F0;}
.con5{background-color:#E93639;}
.con2{font-size:28px;color:#E93639;height:65px;line-height:65px;margin-left:30px;}
.con21{font-size:18px;font-weight:bold;}
.con22{color:#E93639;font-weight:bold;}

