/*xinchouwang.cn CSS Document */

/*CSS Reset*/
body,div,p,ul,li,a{margin:0; padding:0;}
img{border:none;}

#banner{display:block; width:100%; height:300px; margin:0 auto;min-width:1160px;}
#banner img{display:block; width:100%; height:300px;}

.warp-col1,.warp-col2,.warp-col3,.warp-col4.warp-col5{width:100%;min-width:1160px;}
#content{width:1160px;margin:0 auto;padding:90px 0;}
.textnote{text-align:center;padding:0 115px 30px 115px;}
.textnote b{display:block;font-size:22px;font-family:"Microsoft YaHei";color:#DE2E13;}
.textnote img{padding:10px 0;width:324px;height:5px;}
.textnote p{color:#999;font:12px "Microsoft YaHei";line-height:20px;}
.textnote1{text-align:center;padding:0 115px 30px 115px;}
.textnote1 b{display:block;font-size:22px;font-family:"Microsoft YaHei";color:#FFF;}
.textnote1 img{padding:10px 0;width:324px;height:5px;}
.textnote1 p{color:#000;font:12px "Microsoft YaHei";line-height:20px;}
.warp-col2{background-color:#F8F7F7;}
.warp-col4{background:url(../images/hongtu.gif) no-repeat 100% 100%;width:100%;}

.rollBox{width:1160px;overflow:hidden;margin:0 auto;} /*控制外层样式*/
.rollBox .LeftBotton{height:32px;width:32px;background:url(../images/qieh_01.gif) no-repeat;overflow:hidden;float:left;display:inline;margin-left:545px;cursor:pointer;}/*左边按钮*/
.rollBox .RightBotton{height:32px;width:32px;background:url(../images/qieh_02.gif) no-repeat;overflow:hidden;float:left;display:inline;margin-left:5px;cursor:pointer;}/*右边按钮*/
.rollBox .Cont{width:1160px;overflow:hidden;float:left;margin-top:80px;}/*中间内容显示样式*/
.rollBox .ScrCont{width:10000000px;}
.rollBox .Cont .pic{width:293px;float:left;text-align:center;}
.rollBox .Cont .pic img{padding:4px;background:#fff;display:block;margin:0 auto;}
.rollBox .Cont .pic p{line-height:40px;color:#999;font:12px "Microsoft YaHei";padding:30px 0;}
.rollBOx .Cont a{color:#E03E25;background:#E03E25;}
.rollBox .Cont a:link,.rollBox .Cont a:visited{font-size:14px;color:#E03E25;text-decoration:none;}
.rollBox .Cont a:hover{color:#f00;text-decoration:none;}
.rollBox #List1,.rollBox #List2{float:left;}

.morecont{padding-top:80px;}
.morecont a{display:block;margin:0 auto;text-align:center;width:325px;height:50px;border:2px solid #DBDADA;line-height:50px;text-decoration:none;color:#DE2E13;}

