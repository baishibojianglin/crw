@charset "utf-8";
/* CSS Document */
body
{margin:0 auto;
padding:0;
border:0;
font-size:100%;
position:absolute;
bottom:0;
top:0;
right:0;
left:0;
}




/*导航部分css代码*/
.atop1
{
text-decoration:none;
}
.top1
{height:80px;
width:100%;
min-width:1160px;
