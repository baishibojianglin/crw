@charset "utf-8";
@import url("navi.css");

html{overflow-y:scroll;}
body{margin:0; padding:0; font-size:14px; font-family:"Microsoft YaHei";}

.clear{clear:both;}
br.clear{clear:both; margin-top:-15px;}

/*--------------------------------------------Banner-----------------------------------------*/

#banner{display:block; width:100%; height:300px; margin:0 auto;min-width:1160px;}
#banner img{display:block; width:100%; height:300px;}

/* ----------------------------------------------Wrapper------------------------------------- */

div.wrapper{display:block; width:100%;}
.col3{color:#666666;background-color:#FFFFFF;}
.col3a{text-align:center;}
.col3a p{font-size:45px;}
.col3a1 {color:#FF6D50;}
.col3a2 {text-align:center;padding:0;margin:0;}
.col4{height:831px;
    width:100%;
	background-image:url(../images/sb9.gif);}
	
.col4a{position:relative;bottom:30px;text-align:center;}
.col4b{text-align:center;font-size:4em;font-weight:bold;color:#95A3AE;}
.col4c{margin:0;padding:0;height:650px;width:100%;}
.col4c1{height:650px;width:50%;float:left;}
.col4c11{height:650px;width:50%;float:right;}
.col4c3{width:150px;}
.col4c31{width:150px;height:150px;border:1px solid #CACACB;border-radius:100%;-moz-border-radius:100%; /* Old Firefox */}
.col4c3 img{width:142px;height:142px;border:4px solid #FFFFFF;border-radius:100%;-moz-border-radius:100%; /* Old Firefox */float:left;margin-top:30px;}
.col4c30 img{width:142px;height:142px;border:4px solid #FFFFFF;border-radius:100%;-moz-border-radius:100%; /* Old Firefox*/float:left;margin-top:30px;margin-left:100px;}
.col4c3 span{font-size:1em;border:1px solid #666;border-top:none;border-bottom:none;border-right:none;height:194px;background-color:#FFFFFF;}
.col4c4{float:right;margin-right:100px;}
.col4c40{border:2px solid #E3E3E3;border-top:none;border-left:none;border-right:none;margin-top:60px;height:3em;width:300px;margin-left:40px;}
.col4c40 p{letter-spacing:8px;font-size:180%;text-align:center;}
.col4c400{border:3px solid #666;border-top:none;border-left:none;border-right:none;width:40px;margin-left:170px;}
.col4c401{color:#555555;margin:0;}
.col4c401 p{line-height:40px;}
.col4c41{float:right;}
.col4c414{float:right;positon:absolute;bottom:60px;}
.col4b p{color:#95A3AE;border-bottom:2px solid #CCD0D3;width:500px;margin:0 auto;padding-bottom:20px;}
.col5{color:#999999; background-color:#2A2A2A;height:514px;}

/* ----------------------------------------------Generalise------------------------------------- */

#header, .container, #footer{display:block; position:relative; width:1160px; margin:0 auto;}

/* ----------------------------------------------Header------------------------------------- */

#header{padding:10px 0 20px; z-index:1000;}
#header h1, #header p, #header ul{margin:0; padding:0; list-style:none; line-height:normal;}
#header #logo{display:block; float:left; margin-top:7px; overflow:hidden;}
#header #logo h1 a{font-size:46px; color:#999999; background-color:#000000;}
#header h1 strong{font-weight:normal; color:#FFFF00; background-color:#000000;}
#header #logo p{text-align:right; margin-top:5px; font-family:Arial, Helvetica, sans-serif; text-transform:lowercase;}

/* ----------------------------------------------Content------------------------------------- */

.container{padding:0;}
.content{display:block;}

/* ----------------------------------------------Homepage Specific------------------------------------- */

#topstory{display:block;width:100%;height:329px;margin-top:80px;}
#topstory h3, #latestnews h3{margin:0; padding:0;color:#95A3AE;font-family:"Microsoft YaHei";}
#topstory ul{margin:0; padding:0; list-style:none; display:inline;float:left;font-size:14px;}
#topstory li{display:block;width:430px;margin:0; padding:0;line-height:28px;}
#topstory li.last{margin-right:0;}
#topstory .readmore{font-weight:bold; text-align:left;}
.lastt{border:1px solid #666;border-top:none;border-bottom:none;border-right:none;
width:1px;
height:194px; 
float:left;
margin:0 148px;
padding:0;
}

