/* ----------------------------------------------Generalise------------------------------------- */

#header, .container, #footer{display:block; position:relative; width:1160px; margin:0 auto;}

/* ----------------------------------------------Header------------------------------------- */

#header{padding:10px 0 20px; z-index:1000;}
#header h1, #header p, #header ul{margin:0; padding:0; list-style:none; line-height:normal;}
#header #logo{display:block; float:left; margin-top:7px; overflow:hidden;}
#header #logo h1 a{font-size:46px; color:#999999; background-color:#000000;}
#header h1 strong{font-weight:normal; color:#FFFF00; background-color:#000000;}
#header #logo p{text-align:right; margin-top:5px; font-family:Arial, Helvetica, sans-serif; text-transform:lowercase;}

/* ----------------------------------------------Content------------------------------------- */

.container{padding:0;}
.content{display:block;width:100%;height:600px;}
#con1{float:left;width:265px;text-align:center;margin-top:100px;}
#con1 p,#con2 p,#con3 p{color:#000000;text-align:center;font-size:16px;font-family:"Microsoft YaHei";font-weight:bold;}
#con1 span,#con2 span,#con3 span{color:#BCBCBC; font-family:"Microsoft YaHei";text-align:center;line-height:28px;}
#con2{width:265px;float:left;text-align:center;margin-left:150px;margin-top:100px;}
#con3{float:right;margin-top:100px;}
.con4{color:#BCBCBC; font-family:"Microsoft YaHei";text-align:center;line-height:28px;}
